{
  "browser_action": {
    "default_title": "BARTinTime",
    "default_icon": "Images/icon16.png",
    "default_popup": "bit.html"
  },
  "description": "Chrome Extension for checking BART train times",

  "icons": {
    "16": "Images/icon16.png",
    "48": "Images/icon48.png",
    "128": "Images/icon128.png"
  },
  "manifest_version": 2,
  "name": "BARTinTime",
  "version": "0.0.0"
  "permissions": [
    "http://api.bart.gov/*"
   ]
}
